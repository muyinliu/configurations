            }
          },
          "to": [
            {
              "key_code": "caps_lock"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "right_shift"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f4",
            "modifiers": {
              "mandatory": [],
              "optional": []
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/setvolume 3 && /usr/local/bin/terminal-notifier -title \"setvolume to 3\""
            }
          ]
        }
      ]
    }
  ]
}
