      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_control",
                "left_shift"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "shell_command": "if [[ ! -f /tmp/A.txt ]]; then touch /tmp/A.txt; fi; if [[ ! -f /tmp/B.txt ]]; then touch /tmp/B.txt; fi; /usr/local/bin/ksdiff /tmp/A.txt /tmp/B.txt"
            }
          ]
        }
      ]
